{
  "expo": {
    "name": "Falcon Mobile",
    "slug": "falcon-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "falconmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "sdkVersion": "53.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spacedwog.falconmobile",
      "infoPlist": {
        "NSCameraUsageDescription": "Este app precisa acessar a câmera para mostrar o vídeo ao vivo",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.spacedwog.falconmobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "adcce4c3-b878-4f01-a6cf-0fca811ad2d4"
      }
    },
    "owner": "spacedwog"
  }
}
